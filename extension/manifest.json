{
  "name": "Keyxtractor",
  "description": "Extract and highlights keywords in webpages to help read", 
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "http://localhost:27896/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Keyxtract it"
  },
  "manifest_version": 2
}
